<Application x:Class="Lab5_ProductCategories.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Lab5_ProductCategories"
             Startup="Application_Startup">
    <!--StartupUri="MainWindow.xaml" REMOVED to avoid double windows. -->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="NorthwindLogo" UriSource="/Assets/compass.png" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
